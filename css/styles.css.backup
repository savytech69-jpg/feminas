    }

    .hero-content-centered {
        max-width: 100%;
        gap: 12px;
    }

    .hero-badge {
        font-size: 0.75rem;
        padding: 7px 18px;
    }

    .hero-title-new {
        font-size: 2.2rem;
    }

    .hero-subtitle-new {
        font-size: 1rem;
        line-height: 1.55;
    }

    .hero-cta-group {
        flex-direction: column;
        width: 100%;
        max-width: 300px;
        gap: 10px;
        margin-top: 8px;
    }

    .btn-hero-primary,
    .btn-hero-outline {
        width: 100%;
        justify-content: center;
        padding: 15px 30px;
        font-size: 0.95rem;
    }

    .section-title-new {
        font-size: 2.2rem;
    }

    .service-tabs-new {
        flex-direction: column;
        gap: 10px;
    }

    .tab-btn-new {
        width: 100%;
        justify-content: center;
    }

    .makeup-grid-new {
        grid-template-columns: 1fr;
    }

    .bridal-services-grid-new {
        grid-template-columns: 1fr;
    }

    .signature-looks-grid {
        grid-template-columns: 1fr;
    }

    .addons-grid-new {
        grid-template-columns: 1fr;
    }

    .facials-grid-new {
        grid-template-columns: 1fr;
    }

    .specialized-grid-new {
        grid-template-columns: 1fr;
    }

    .specialized-card-new {
        flex-direction: column;
    }

    .specialized-img {
        width: 100%;
        height: 200px;
    }

    .waxing-grid-new {
        grid-template-columns: 1fr;
    }

    .nails-showcase-grid {
        grid-template-columns: 1fr;
    }

    .nail-card-lg {
        grid-column: span 1;
        grid-row: span 1;
    }

    .nail-img-lg {
        height: 250px;
    }

    .cta-content-new h2 {
        font-size: 2rem;
    }

    .cta-buttons-new {
        flex-direction: column;
        align-items: center;
    }

    .btn-cta-primary,
    .btn-cta-secondary {
        width: 100%;
        max-width: 300px;
        justify-content: center;
    }
}

/* ===== CONTACT PAGE STYLES ===== */

/* Contact Hero */
.contact-hero {
    position: relative;
    height: 50vh;
    min-height: 400px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: linear-gradient(135deg, #1a0033 0%, #2d0a4e 50%, #1a0033 100%);
    overflow: hidden;
}

.contact-hero .hero-overlay {
    position: absolute;
    inset: 0;
    background: radial-gradient(ellipse at top, rgba(138, 43, 226, 0.15) 0%, transparent 50%),
        radial-gradient(ellipse at bottom, rgba(75, 0, 130, 0.2) 0%, transparent 50%);
}

.contact-hero .hero-content {
    position: relative;
    z-index: 2;
    text-align: center;
    max-width: 800px;
    padding: 0 20px;
}

.contact-hero .hero-badge {
    display: inline-block;
    padding: 10px 28px;
    background: linear-gradient(135deg, rgba(138, 43, 226, 0.15), rgba(186, 85, 211, 0.1));
    border: 2px solid rgba(186, 85, 211, 0.4);
    border-radius: 50px;
    color: #E0B0FF;
    font-size: 0.85rem;
    font-weight: 700;
    letter-spacing: 3px;
    text-transform: uppercase;
    margin-bottom: 24px;
}

.contact-hero .hero-title {
    font-size: 4.5rem;
    font-weight: 900;
    line-height: 1;
    color: #fff;
    font-family: var(--font-heading);
    margin-bottom: 20px;
    text-transform: uppercase;
    background: linear-gradient(135deg, #fff 0%, #E0B0FF 50%, #BA55D3 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
}

.contact-hero .hero-subtitle {
    font-size: 1.2rem;
    color: rgba(224, 176, 255, 0.9);
    line-height: 1.6;
}

/* Contact Main */
.contact-main {
    padding: 100px 0;
    background: var(--primary-bg);
}

.contact-grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 50px;
    align-items: start;
}

/* Contact Form Section */
.contact-form-section {
    background: linear-gradient(135deg, rgba(255, 255, 255, 0.05), rgba(255, 255, 255, 0.02));
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(186, 85, 211, 0.2);
    border-radius: 24px;
    padding: 50px;
    box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
}

.form-title {
    font-size: 2.5rem;
    font-weight: 800;
    color: #fff;
    font-family: var(--font-heading);
    margin-bottom: 12px;
    background: linear-gradient(135deg, #fff 0%, #E0B0FF 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
}

.form-subtitle {
    font-size: 1rem;
    color: rgba(224, 176, 255, 0.7);
    margin-bottom: 40px;
}

/* Form Styles */
.contact-form .form-row {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 20px;
    margin-bottom: 20px;
}

.contact-form .form-group {
    margin-bottom: 20px;
}

.contact-form label {
    display: block;
    font-size: 0.9rem;
    font-weight: 600;
    color: rgba(224, 176, 255, 0.9);
    margin-bottom: 8px;
    letter-spacing: 0.5px;
}

.contact-form input,
.contact-form select,
.contact-form textarea {
    width: 100%;
    padding: 14px 18px;
    background: rgba(255, 255, 255, 0.05);
    border: 1px solid rgba(186, 85, 211, 0.3);
    border-radius: 12px;
    color: #fff;
    font-size: 1rem;
    font-family: var(--font-body);
    transition: all 0.3s ease;
}

.contact-form input:focus,
.contact-form select:focus,
.contact-form textarea:focus {
    outline: none;
    border-color: rgba(186, 85, 211, 0.8);
    background: rgba(255, 255, 255, 0.08);
    box-shadow: 0 0 20px rgba(138, 43, 226, 0.3);
}

.contact-form input::placeholder,
.contact-form textarea::placeholder {
    color: rgba(224, 176, 255, 0.4);
}

.contact-form textarea {
    resize: vertical;
    min-height: 120px;
}

/* Submit Button */
.btn-submit {
    width: 100%;
    padding: 16px 32px;
    background: linear-gradient(135deg, #8A2BE2 0%, #BA55D3 100%);
    border: none;
    border-radius: 12px;
    color: #fff;
    font-size: 1.1rem;
    font-weight: 700;
    font-family: var(--font-heading);
    text-transform: uppercase;
    letter-spacing: 1px;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    transition: all 0.3s ease;
    box-shadow: 0 4px 20px rgba(138, 43, 226, 0.4);
}

.btn-submit:hover {
    transform: translateY(-2px);
    box-shadow: 0 6px 30px rgba(138, 43, 226, 0.6);
    background: linear-gradient(135deg, #9A3BF2 0%, #CA65E3 100%);
}

/* Map & Info Section */
.contact-info-section {
    display: flex;
    flex-direction: column;
    gap: 30px;
}

.map-container {
    height: 400px;
    border-radius: 20px;
    overflow: hidden;
    border: 2px solid rgba(186, 85, 211, 0.3);
    box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
}

.map-container iframe {
    width: 100%;
    height: 100%;
}

/* Info Cards */
.info-cards {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 20px;
}

.info-card {
    background: linear-gradient(135deg, rgba(138, 43, 226, 0.15), rgba(75, 0, 130, 0.1));
    backdrop-filter: blur(10px);
    border: 1px solid rgba(186, 85, 211, 0.3);
    border-radius: 16px;
    padding: 24px;
    display: flex;
    align-items: flex-start;
    gap: 16px;
    transition: all 0.3s ease;
}

.info-card:hover {
    transform: translateY(-4px);
    border-color: rgba(186, 85, 211, 0.6);
    box-shadow: 0 8px 24px rgba(138, 43, 226, 0.3);
}

.info-icon {
    width: 50px;
    height: 50px;
    border-radius: 12px;
    background: linear-gradient(135deg, rgba(138, 43, 226, 0.3), rgba(186, 85, 211, 0.2));
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
}

.info-icon i {
    font-size: 1.5rem;
    color: #E0B0FF;
}

.info-content h3 {
    font-size: 1.1rem;
    font-weight: 700;
    color: #fff;
    margin-bottom: 8px;
    font-family: var(--font-heading);
}

.info-content p {
    font-size: 0.95rem;
    color: rgba(224, 176, 255, 0.8);
    line-height: 1.6;
}

/* WhatsApp Card */
.whatsapp-card {
    grid-column: 1 / -1;
    background: linear-gradient(135deg, rgba(37, 211, 102, 0.15), rgba(37, 211, 102, 0.05));
    border-color: rgba(37, 211, 102, 0.4);
}

.whatsapp-card .info-icon {
    background: linear-gradient(135deg, rgba(37, 211, 102, 0.3), rgba(37, 211, 102, 0.2));
}

.whatsapp-card .info-icon i {
    color: #25D366;
}

.whatsapp-link {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    color: #25D366;
    font-weight: 600;
    text-decoration: none;
    transition: all 0.3s ease;
}

.whatsapp-link:hover {
    gap: 12px;
    color: #20BA5A;
}

/* Responsive */
@media (max-width: 1024px) {
    .contact-grid {
        grid-template-columns: 1fr;
        gap: 40px;
    }

    .contact-form-section {
        padding: 40px;
    }
}

@media (max-width: 768px) {
    .contact-hero .hero-title {
        font-size: 3rem;
    }

    .contact-form .form-row {
        grid-template-columns: 1fr;
        gap: 0;
    }

    .contact-form-section {
        padding: 30px 20px;
    }

    .info-cards {
        grid-template-columns: 1fr;
    }

    .map-container {
        height: 300px;
    }
}

@media (max-width: 480px) {
    .contact-hero .hero-title {
        font-size: 2.5rem;
    }

    .form-title {
        font-size: 2rem;
    }
}