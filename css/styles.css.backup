}

.service-tile {
    position: relative;
    background: linear-gradient(135deg, rgba(15, 5, 35, 0.95) 0%, rgba(42, 14, 69, 0.95) 100%);
    border-radius: 20px;
    overflow: hidden;
    border: 1px solid rgba(255, 215, 0, 0.2);
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
