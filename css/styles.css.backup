}

/* Asymmetric Service Grid (2025 Trend) */
.services-asymmetric {
    padding: 60px 0;
}

.services-grid-modern {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
    margin-top: 50px;
    align-items: stretch;
}

.service-tile.spa-wide {
